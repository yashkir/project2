<h4>Add a Transaction</h4>
<div class="row">
  <form class="col s12" action="/transactions" method="post">
    <div class="row">
      <div class="input-field col s6">
        <label for="date">date</label>
        <input placeholder="today" type="date" name="date" id="date">
      </div>
      <div class="input-field col s6">
        <label for="title">title</label>
        <input type="text" name="title" id="title">
      </div>
    </div>

    <div class="row">
      <div class="input-field col s6">
        <label for="account-1">account name</label>
        <input type="text" name="account-1" id="account-1">
      </div>
      <div class="input-field col s6">
        <label for="amount-1">amount</label>
        <input type="text" name="amount-1" id="amount-1">
      </div>
    </div>

    <div class="row">
      <div class="input-field col s6">
        <label for="account-2">account name</label>
        <input type="text" name="account-2" id="account-2">
      </div>
      <div class="input-field col s6">
        <label for="amount-2">amount</label>
        <input type="text" name="amount-2" id="amount-2">
      </div>
    </div>

    <input class="btn" type="submit" value="Add">
  </form>
</div>
